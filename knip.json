{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "client/index.js",
    "server/index.js",
    "server/start.js",
    "config/**/*.js"
  ],
  "project": [
    "client/**/*.{js,jsx}",
    "server/**/*.js",
    "config/**/*.js",
    "src/**/*.{js,jsx}"
  ],
  "ignore": ["client/assets/default/index.js", "client/assets/mobile/index.js"],
  "ignoreExportsUsedInFile": true
}
