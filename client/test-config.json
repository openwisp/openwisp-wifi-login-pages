[
  {
    "name": "Default",
    "slug": "default",
    "default_language": "en",
    "auto_login": true,
    "settings": {
      "mobile_phone_verification": false
    },
    "components": {
      "status_page": {
        "links": [
          {
            "text": "Change your password",
            "url": "/{orgSlug}/change-password/"
          }
        ]
      },
      "logout": {
        "title": {
          "en": "Logout"
        },
        "content": {
          "text": {
            "en": "Logout successfull"
          }
        },
        "login_again": {
          "text": {
            "en": "Log in again"
          }
        }
      },
      "captive_portal_login_form": {
        "method": "post",
        "action": "http://localhost/",
        "fields": {
          "username": "auth_user",
          "password": "auth_pass"
        },
        "macaddr_param_name": "macaddr",
        "additional_fields": [
          {
            "name": "zone",
            "value": "zone_name"
          },
          {
            "name": "redirurl",
            "value": "http://localhost:8080/default/status"
          },
          {
            "name": "accept",
            "value": "accept"
          }
        ]
      },
      "captive_portal_logout_form": {
        "method": "post",
        "action": "http://localhost:8081",
        "fields": {
          "id": "logout_id"
        },
        "additional_fields": [],
        "logout_by_session": true
      },
      "registration_form": {
        "input_fields": {
          "username": {
            "type": "text",
            "pattern": "[a-zA-Z@.+\\-_\\d]{1,150}"
          },
          "email": {
            "type": "email",
            "pattern": ".+@.+\\..+"
          },
          "password": {
            "type": "password",
            "pattern": ".{6,}"
          },
          "password_confirm": {
            "type": "password"
          },
          "phone_number": {
            "type": "tel"
          },
          "first_name": {
            "type": "text",
            "pattern": "[a-zA-Z@.+\\-_\\d]{1,150}",
            "setting": "disabled"
          },
          "last_name": {
            "type": "text",
            "pattern": "[a-zA-Z@.+\\-_\\d]{1,150}",
            "setting": "disabled"
          },
          "location": {
            "type": "text",
            "pattern": "[a-zA-Z@.+\\-_\\d]{1,150}",
            "setting": "disabled"
          },
          "birth_date": {
            "type": "date",
            "setting": "disabled"
          },
          "city": {
            "type": "text"
          },
          "street": {
            "type": "text"
          },
          "tax_number": {
            "type": "text",
            "pattern": "[a-zA-Z@.+\\-_\\d]{1,150}"
          },
          "country": {
            "type": "text"
          },
          "zipcode": {
            "type": "number"
          }
        },
        "additional_info_text": true,
        "links": {
          "login": true
        }
      }
    },
    "privacy_policy": {
      "title": {
        "en": "privacy policy"
      },
      "content": {
        "en": "..."
      }
    },
    "terms_and_conditions": {
      "title": {
        "en": "terms and conditions"
      },
      "content": {
        "en": "..."
      }
    }
  },
