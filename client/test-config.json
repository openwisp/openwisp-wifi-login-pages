[
  {
    "name": "Default",
    "slug": "default",
    "default_language": "en",
    "auto_login": true,
    "settings": {
      "mobile_phone_verification": false
    },
    "components": {
      "status_page": {
        "content": {
          "en": "WiFi Login Successful!\nYou can now use the internet.\nYou may leave this page open in case you want to log out.\n"
        },
        "user_info": {
          "status": {
            "text": {
              "en": "Status"
            },
            "value": {
              "en": "Logged in"
            }
          },
          "email": {
            "text": {
              "en": "Email"
            }
          },
          "username": {
            "text": {
              "en": "Username"
            }
          },
          "phone_number": {
            "text": {
              "en": "Phone number"
            }
          }
        },
        "links": [
          {
            "text": {
              "en": "Change your password"
            },
            "url": "/{orgSlug}/change-password/"
          }
        ],
        "buttons": {
          "logout": {
            "text": {
              "en": "Logout"
            }
          }
        }
      },
      "captive_portal_login_form": {
        "method": "post",
        "action": "http://localhost/",
        "fields": {
          "username": "auth_user",
          "password": "auth_pass"
        },
        "macaddr_param_name": "macaddr",
        "additional_fields": [
          {
            "name": "zone",
            "value": "zone_name"
          },
          {
            "name": "redirurl",
            "value": "http://localhost:8080/default/status"
          },
          {
            "name": "accept",
            "value": "accept"
          }
        ]
      },
      "captive_portal_logout_form": {
        "method": "post",
        "action": "http://localhost:8081",
        "fields": {
          "id": "logout_id"
        },
        "additional_fields": []
      },
      "registration_form": {
        "input_fields": {
          "username": {
            "type": "text",
            "pattern": "[a-zA-Z@.+\\-_\\d]{1,150}",
            "pattern_description": {
              "en": "only letters, numbers, and @/./+/-/_ characters"
            },
            "placeholder": {
              "en": "enter username"
            },
            "label": {
              "en": "username"
            }
          },
          "email": {
            "type": "email",
            "pattern": ".+@.+\\..+",
            "pattern_description": {
              "en": null
            },
            "placeholder": {
              "en": "email address"
            },
            "label": {
              "en": "email"
            }
          },
          "password": {
            "type": "password",
            "pattern": ".{6,}",
            "pattern_description": {
              "en": "password must be a minimum of 6 characters"
            },
            "placeholder": {
              "en": "password"
            },
            "label": {
              "en": "password"
            }
          },
          "password_confirm": {
            "type": "password",
            "label": {
              "en": "confirm password"
            },
            "placeholder": {
              "en": "confirm password"
            },
            "label": {
              "en": "confirm"
            }
          },
          "phone_number": {
            "placeholder": {
              "en": "enter mobile phone number"
            },
            "label": {
              "en": "mobile phone number"
            }
          }
        },
        "buttons": {
          "register": {
            "label": null,
            "text": {
              "en": "sign up"
            }
          }
        },
        "additional_info_text": {
          "en": "By signing up, you accept the {terms_and_conditions} and the {privacy_policy} of this WiFi service. "
        },
        "links": {
          "login": {
            "en": "Already have an account? Log in"
          }
        }
      }
    },
    "privacy_policy": {
      "title": {
        "en": "privacy policy"
      },
      "content": {
        "en": "..."
      }
    },
    "terms_and_conditions": {
      "title": {
        "en": "terms and conditions"
      },
      "content": {
        "en": "..."
      }
    }
